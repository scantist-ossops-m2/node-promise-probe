{
	"compilerOptions": {
	  "target": "es2020",
	  "module": "commonjs",
  
	  "sourceMap": true,
	  "strict": false,
  
	  "strictBindCallApply": true,
	  "strictFunctionTypes": true,
	  "noImplicitThis": true,
  
	  "moduleResolution": "node",
  
	  "esModuleInterop": true,
  
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	}
  }
  